        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivDoubleTable3"
        app:srcCompat="@drawable/mesadoble_vertical" />

    <ImageView
        android:id="@+id/ivDoubleTable8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivDoubleTable7"
        app:srcCompat="@drawable/mesadoble_vertical" />

    <ImageView
        android:id="@+id/ivQuadrupleTable1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:contentDescription="9"
        app:layout_constraintEnd_toStartOf="@+id/ivQuadrupleTable3"
        app:layout_constraintTop_toTopOf="@+id/ivQuadrupleTable3"
        app:srcCompat="@drawable/mesacuadruple" />

    <ImageView
        android:id="@+id/ivQuadrupleTable2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="10"
        app:layout_constraintEnd_toEndOf="@+id/ivQuadrupleTable1"
        app:layout_constraintTop_toBottomOf="@+id/ivQuadrupleTable1"
        app:srcCompat="@drawable/mesacuadruple" />

    <ImageView
        android:id="@+id/ivQuadrupleTable3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:contentDescription="11"
        app:layout_constraintEnd_toStartOf="@+id/ivDoubleTable7"
        app:layout_constraintTop_toTopOf="@+id/ivDoubleTable7"
        app:srcCompat="@drawable/mesacuadruple" />

    <ImageView
        android:id="@+id/ivQuadrupleTable4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="12"
        app:layout_constraintEnd_toEndOf="@+id/ivQuadrupleTable3"
        app:layout_constraintTop_toBottomOf="@+id/ivQuadrupleTable3"
