        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mesadoble_horizontal" />

    <ImageView
        android:id="@+id/ivDoubleTable5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:contentDescription="5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ivDoubleTable6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivDoubleTable4"
        app:srcCompat="@drawable/mesadoble_horizontal" />

    <ImageView
        android:id="@+id/ivDoubleTable6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:contentDescription="6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivDoubleTable5"
        app:srcCompat="@drawable/mesadoble_horizontal" />

    <ImageView
