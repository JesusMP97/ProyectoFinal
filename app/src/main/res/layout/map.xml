        android:layout_marginStart="20dp"
        android:layout_marginTop="32dp"
        android:contentDescription="13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mesaredonda" />

    <ImageView
        android:id="@+id/ivOctaTable2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="70dp"
        android:contentDescription="14"
        app:layout_constraintStart_toEndOf="@+id/ivOctaTable1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mesaredonda" />

    <ImageView
        android:id="@+id/ivOctaTable3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:contentDescription="15"
        app:layout_constraintStart_toStartOf="@+id/ivOctaTable1"
        app:layout_constraintTop_toBottomOf="@+id/ivOctaTable1"
        app:srcCompat="@drawable/mesaredonda" />

    <ImageView
        android:id="@+id/ivBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="46dp"
        android:contentDescription="16"
        android:rotation="180"
        app:layout_constraintBottom_toTopOf="@+id/ivDoubleTable4"
        app:layout_constraintEnd_toStartOf="@+id/ivBar2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/silla" />

    <ImageView
        android:id="@+id/ivBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="17"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="@+id/ivBar1"
        app:layout_constraintEnd_toStartOf="@+id/ivBar3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivBar1"
        app:srcCompat="@drawable/silla" />

    <ImageView
        android:id="@+id/ivBar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:contentDescription="18"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="@+id/ivBar2"
        app:layout_constraintEnd_toStartOf="@+id/ivBar4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivBar2"
        app:srcCompat="@drawable/silla" />

    <ImageView
        android:id="@+id/ivBar4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:contentDescription="19"
        android:rotation="180"
        app:layout_constraintStart_toEndOf="@+id/ivBar3"
        app:layout_constraintTop_toBottomOf="@+id/ivBar5"
        app:srcCompat="@drawable/silla" />

    <ImageView
        android:id="@+id/ivBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:contentDescription="20"
        android:rotation="180"
        app:layout_constraintBottom_toTopOf="@+id/ivBar3"
        app:layout_constraintStart_toStartOf="@+id/ivBar4"
        app:srcCompat="@drawable/silla" />

</androidx.constraintlayout.widget.ConstraintLayout>